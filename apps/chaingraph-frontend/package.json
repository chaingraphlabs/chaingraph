    "directory": "./apps/chaingraph-frontend"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/lib/exports.d.ts",
      "import": "./dist/lib/chaingraph-frontend.mjs",
      "default": "./dist/lib/chaingraph-frontend.js"
    },
    "./store": {
      "types": "./dist/lib/store.d.ts",
