  "style": "./dist/lib/chaingraph-frontend.css",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "vite",
    "dev-force": "vite --force",
    "build": "npm run build:app && npm run build:lib",
    "build:app": "tsc -b && vite build --config vite.app.config.ts",
    "build:lib": "vite build --config vite.lib.config.ts && tsc -p tsconfig.lib.json",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-types": "tsc -b --noEmit"
