# Event Stream Configuration
EVENT_STREAM_PORT=3001
EVENT_STREAM_WS_PATH=/ws
EVENT_STREAM_BUFFER_SIZE=1000

# Logging Configuration
LOG_LEVEL=info
NODE_ENV=development