{
  "name": "@badaitech/badai-api",
  "type": "module",
  "version": "0.1.28",
  "private": false,
  "description": "Bad AI GraphQL API client",
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com//badaitech/chaingraph.git",
    "directory": "./packages/badai-api"
  },
